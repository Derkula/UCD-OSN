universe = vanilla

Process = 17

Requirements    = HAS_SINGULARITY == TRUE
request_cpus    = 1
request_memory  = 1 GB
request_disk    = 4 GB


OSDF_LOCATION = osdf:///ospool/uc-shared/public/UCDenver_Roberts
OSDF_DATA = osdf:///ospool/ap21/data/derkula

+SingularityImage = "osdf:///ospool/uc-shared/public/UCDenver_Roberts/container/cdms4.0.sif"


executable = test.sh

transfer_input_files    = env.sh
